@page "/resultsquestionnaire/{questionnaireId}"
@inject IJSRuntime JSRuntime

<h1>Edit</h1>
<div id="resultsQuestionnaire">

    <NavLink class="nav-link" href="questionnaire">
        <span class="oi oi-list-rich" aria-hidden="true"></span> Back to Discovery
    </NavLink>

    <div class="sv_main sv_frame sv_default_css">
        <div class="sv_custom_header"></div>
        <div class="sv_container">
            <div class="sv_header">
                <h3 data-bind="text: surveyId + ' results'"></h3>
            </div>
            <div class="sv_body">
                <table id="resultsTable" class="table table-striped table-bordered">
                </table>
            </div>
        </div>
    </div>
</div>

@code {
    [Parameter] public string questionnaireId { get; set; }

    protected void OnAfterRenderAsync()
    {
        JSRuntime.InvokeAsync<object>(
        "surveyResultsFunctions.loadSurvey", questionnaireId);
    }
}
